        "flow-copy-source": "^2.0.2",
        "flow-coverage-report": "^0.6.0",
        "jest": "^23.6.0",
        "jest-enzyme": "^6.1.2",
        "prettier": "^1.14.3",
        "react": "^16.8.0",
        "react-dom": "^16.8.0",
        "rollup": "^0.66.1",
        "rollup-plugin-babel": "^4.0.3",
        "rollup-plugin-commonjs": "^9.1.8",
